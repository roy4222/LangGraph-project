{
  "name": "langgraph",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:all": "npm run dev & npm run dev:worker",
    "dev:worker": "cd workers/api && npm run dev",
    "build": "next build",
    "build:all": "npm run build && npm run build:worker",
    "build:worker": "cd workers/api && npm run build",
    "start": "next start",
    "lint": "eslint",
    "deploy": "npm run build && wrangler pages deploy .next",
    "deploy:worker": "cd workers/api && npm run deploy"
  },
  "dependencies": {
    "next": "16.0.3",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
